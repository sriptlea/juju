	is_open = true,
	flags = {
		loaded_scripts = {}
	},
	active_keybind = nil,
	active_colorpicker = nil,
	accent_color = colorfromrgb(153, 196, 39),
	on_accent_change = signal.new(),
	blocked = false,
	animation_speed = 0.15,
	keybinds = {}
}

